<div class="container">
  <div class="row">
    <div class="col-md-6">
      <h4><span class="icon ion-navicon"> </span> Posts</h4>
    </div>
    <div class="col-md-6">
    </div>
  </div>
  {{#each}}
    {{#if private}}
      {{#if session.isAuthenticated}}
        <h4>
          {{#link-to 'posts.view' id}}{{title}}{{/link-to}}
          <small>
          / {{ago created_at}}
          <span class="label label-danger">Private</span>
          </small>
        </h4>
      {{/if}}
    {{else}}
      <h4>
        {{#link-to 'posts.view' id}}{{title}}{{/link-to}}
        <small>
        / {{ago created_at}}
        </small>
      </h4>
    {{/if}}
  {{/each}}
</div>
